<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FinalApp.Resources.Styles.DarkTheme">

    <Style x:Key="PageStyle" TargetType="Page">
        <Setter x:Name="PageBackground" Property="Background" Value="#1a1a1a"/>
    </Style>

    <Style x:Key="LabelPrimary" TargetType="Label">
        <Setter x:Name="TextColorPrimary" Property="TextColor" Value="White"/>
    </Style>

    <Color x:Key="PrimaryColor">#1a1a1a</Color>
    <Color x:Key="SecondaryColor">#e90052</Color>

</ResourceDictionary>
